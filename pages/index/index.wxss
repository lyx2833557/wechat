/**index.wxss**/
.container{
  padding: 56rpx 30rpx 0;
}
.container .header{
  text-align: center;
  margin-bottom: 112rpx;
}
.search_box{
  width: 100%;
  height:70rpx ;
  border-radius: 35rpx;
  overflow: hidden;
}
.container .search_box input{
  width: 100%;
  height: 100%;
  background-color: #f7f7f7;
  padding-left: 80rpx;
  box-sizing: border-box;

}
.list_box{
  margin-top: 30rpx;

}
.list_item{
  display: flex;
  margin-bottom: 20rpx;
}
.list_item .img_box{
  width: 208rpx;
  height: 208rpx;
  border-radius: 8rpx;
  overflow: hidden;
  flex-shrink: 0;
}
.list_item .img_box image{
  width: 100%;
  height: 100%;

}
.song_list{
  /* width: 450rpx; */
  padding: 20rpx 0;
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  overflow: hidden;
  flex: 1;
 
}
.list_item .song_item{
  /* display: flex; */
  font-size: 28rpx;
  color: #333333;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap; 

  /* height: 40rpx; */

}
